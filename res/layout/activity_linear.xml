<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="${relativePackage}.${activityClass}" >

   <EditText
        android:id="@+id/et1"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:ems="10"
        android:textSize="20sp" >
    </EditText>

    <EditText
        android:id="@+id/et2"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/et1"
        android:layout_below="@+id/et1"
        android:ems="10"
        android:textSize="20sp" />

    <EditText
        android:id="@+id/et3"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/et2"
        android:layout_below="@+id/et2"
        android:ems="10"
        android:textSize="20sp" />

    <EditText
        android:id="@+id/et4"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/et3"
        android:layout_below="@+id/et3"
        android:ems="10"
        android:textSize="20sp" />

    <EditText
        android:id="@+id/et5"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/et4"
        android:layout_below="@+id/et4"
        android:ems="10"
        android:textSize="20sp" />

    <EditText
        android:id="@+id/et6"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/et5"
        android:layout_below="@+id/et5"
        android:ems="10"
        android:textSize="20sp" />

    <EditText
        android:id="@+id/et7"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/et6"
        android:layout_below="@+id/et6"
        android:ems="10"
        android:textSize="20sp" />

    <EditText
        android:id="@+id/et8"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/et7"
        android:layout_below="@+id/et7"
        android:ems="10"
        android:textSize="20sp" />

    <EditText
        android:id="@+id/et9"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/et8"
        android:layout_below="@+id/et8"
        android:ems="10"
        android:textSize="20sp" />

    <EditText
        android:id="@+id/et10"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:layout_alignLeft="@+id/et9"
        android:layout_below="@+id/et9"
        android:ems="10"
        android:textSize="20sp" />

</RelativeLayout>